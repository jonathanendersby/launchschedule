{
  "manifest_version": 2,

  "name": "Launch Schedule",
  "description": "Upcoming rocket launches.",
  "version": "1.4",

  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  
  "icons": { "128": "icon128.png" },  
  
  "background": {
      "scripts": ["jquery-2.1.3.min.js", "background.js"]
  },
  
  "permissions": [
    "http://underground.co.za/"
  ]
}